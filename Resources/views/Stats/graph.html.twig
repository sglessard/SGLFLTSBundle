<h3>Tendence, du {{ graph_from }} au {{ graph_to }}</h3>


    <svg width="100%" height="400" viewBox="0 0 13 100" class="chart">

  <polyline
     fill="none"
     stroke="#0074d9"
     stroke-width="2"
     points="
{% set i = 0 %}
{% for month in months %}
    {% set i = i + 1 %}
   {{ ++i }},{{ month.hours }}
{% endfor %}
     "/>
    <polyline
     fill="none"
     stroke="#ff74d9"
     stroke-width="2"
     points="
{% set i = 0 %}
{% for month in months %}
    {% set i = i + 1 %}
   {{ ++i }},{{ month.money / 100 }}
{% endfor %}
     "/>
</svg>